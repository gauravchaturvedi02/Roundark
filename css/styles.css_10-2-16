.more-work-bg{background:#efefef; margin-top:80px;}
.more-work-row{width:100%; float:left; padding:70px 0px;}
.more-work-row h3{color:#424242; font-size:40px; font-family:'Roboto Slab', serif; margin-bottom:50px; font-weight:100; text-align:center;}
.more-work-row ul{list-style:none;}
.more-work-row ul li {height:400px; width:33.33%; background:#5a71bd; padding:0px; margin:0px; color:#fff; overflow:hidden; float:left; position:relative;}
.more-work-row ul li a{height:100%; display:block;}
.more-work-row ul li a > span.figure{background:#5a71bd; height:100%}
.more-work-row ul li img {width:100%;height:auto;}
.more-work-row ul li span.center {
	-moz-transform:scale(1.3);
	-ms-transform:scale(1.3);
	-webkit-transform:scale(1.3);
	transform:scale(1.3);
	-moz-transition:transform 0.4s;
	-o-transition:transform 0.4s;
	-webkit-transition:transform 0.4s;
	transition:transform 0.4s
}
.more-work-row ul li img {
 height: 400px;
  width: 100%;
  -webkit-transition: all 1s ease;
     -moz-transition: all 1s ease;
       -o-transition: all 1s ease;
      -ms-transition: all 1s ease;
          transition: all 1s ease;
}
.more-work-row ul li .figure:hover .center {
	-moz-transform:scale(1);
	-ms-transform:scale(1);
	-webkit-transform:scale(1);
	transform:scale(1)
}
.more-work-row ul li:hover img {width:500px; height:500px;}
.more-work-row ul li .figure:hover .center {opacity:1;}
.more-work-row ul li .figure:hover img {opacity:0.1;}
.more-work-row ul li span.proname{width:100%; position:absolute; z-index:9994; text-align:center; left:0px; top:0px; margin-top:45%; color:#fff; font-size:32px; font-weight:600; opacity:0;}
.more-work-row ul li .figure:hover .center span{width:100%; position:absolute; z-index:999; text-align:center; left:0px; top:0px; margin-top:45%; color:#fff; font-size:32px; font-weight:600;}
.more-work-row ul li:hover span.proname{width:100%; position:absolute; z-index:9994; text-align:center; left:0px; top:0px; margin-top:45%; color:#fff; font-size:32px; font-weight:600; opacity:1;}

/***case-study-end*****/





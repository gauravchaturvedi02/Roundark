/*----- Tabs -----*/
.tabs {width:100%;display:inline-block;}
/*----- Tab Links -----*/
/* Clearfix */
.tab-links:after {display:block; clear:both;  content:''; }
.tab-links{text-align:center;}
.tab-links li {margin:0px 15px; display:inline-block; list-style:none;  border-radius:15px 15px 0px 0px; khtml-border-radius:15px 15px 0px 0px; moz-border-radius:15px 15px 0px 0px; webkit-border-radius:15px 15px 0px 0px;}
.tab-links li.social{background:#cab14a;}
.tab{ width:100%; float:left;}
.tab#tab1{background:#cab14a;}
.tab-links li.search{background:#818181;}
.tab#tab2{background:#818181;}
.tab-links li.display{background:#5a72ba;}
.tab#tab3{background:#5a72ba;}
.tab-links a {padding:25px 90px; display:inline-block;border-radius:3px 3px 0px 0px;/*background:#7FB5DA;*/font-size:24px;color:#fff;transition:all linear 0.15s; font-weight:700; letter-spacing:2px;}
.tab-links a:hover {background:none;text-decoration:none;}
li.active a, li.active a:hover {/*background:#fff;color:#4c4c4c;*/}
/*----- Content of Tabs -----*/
.tab-content {padding:0px 0px 15px 0px; border-radius:3px;box-shadow:none;background:#fff;}

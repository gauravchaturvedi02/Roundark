/* set all the styles to 0 */
html, body, table, ul, ol, li, p, h1, h2, h3, h4, h5, h6, form, fieldset, a{ 
	margin: 0px;
	padding: 0px;
	border: 0px;
}
/* Firefox will always display a scroll bar */
html{
	min-height: 100%;
	margin-bottom: 1px;
}

html, body{
	height:100%;
}
 img, div, input {behavior: url("iepngfix.htc")}
:focus {outline:0px;}

html, body, table, ul, ol, li, p, h1, h2, h3, h4, h5, h6, form, fieldset, a{ font-family: 'Open Sans', sans-serif;}


